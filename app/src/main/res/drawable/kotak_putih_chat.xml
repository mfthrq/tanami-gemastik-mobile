<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
    <!-- Mengatur sudut radius hanya untuk kanan atas dan kiri atas -->
    <corners
        android:radius="10dp"/>

    <!-- Mengatur gradien warna hijau -->
    <gradient
        android:startColor="#FFFFFF"
        android:endColor="#FFFFFF"
        android:angle="120"/>

    <!-- Menambahkan stroke abu-abu -->
    <stroke
        android:width="1dp"
        android:color="#D6D6D6" /> <!-- Warna abu-abu -->
</shape>
